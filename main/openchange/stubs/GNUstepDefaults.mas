	<key>OCSFolderInfoURL</key>
	<string><% $OCSFolderInfoURL %></string>
	<key>OCSSessionsFolderURL</key>
	<string><% $OCSSessionsFolderURL %></string>
	<key>SOGoProfileURL</key>
	<string><% $SOGoProfileURL %></string>
