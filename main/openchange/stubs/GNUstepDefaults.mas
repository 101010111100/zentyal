	<array>
	    <dict>
		<key>CNFieldName</key>
		<string><% $SOGoUserSources->{CNFieldName} %></string>
		<key>IDFieldName</key>
		<string><% $SOGoUserSources->{IDFieldName} %></string>
		<key>IMAPHostFieldName</key>
		<string><% $SOGoUserSources->{IMAPHostFieldName} %></string>
