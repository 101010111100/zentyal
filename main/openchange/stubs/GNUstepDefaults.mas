		<key>id</key>
		<string><% $SOGoUserSources->{id} %></string>
		<key>isAddressBook</key>
		<string><% $SOGoUserSources->{isAddressBook} %></string>
		<key>port</key>
		<string><% $SOGoUserSources->{port} %></string>
	    </dict>
	</array>
	<key>WOLogFile</key>
	<string><% $WOLogFile %></string>
	<key>WONoDetach</key>
