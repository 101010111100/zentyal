<%args>
    $tunnel
</%args>
ipcp-accept-local
ipcp-accept-remote
%   if ($tunnel->{'primary_ns'}) {
ms-dns  <% $tunnel->{'primary_ns'} %>
%   }
%   if ($tunnel->{'secondary_ns'}) {
ms-dns  <% $tunnel->{'secondary_ns'} %>
%   }
%   if ($tunnel->{'wins_server'}) {
ms-wins <% $tunnel->{'wins_server'} %>
%   }
noccp
auth
crtscts
idle 1800
mtu 1410
mru 1410
nodefaultroute
debug
lock
proxyarp
connect-delay 5000

